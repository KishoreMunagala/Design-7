Time:O(nlogn)
Space:O(1)

class Solution {
    public int hIndex(int[] citations) 
    {
        int n = citations.length;
        Arrays.sort(citations);
        for(int i=0;i<n;i++)
        {
            int temp = n-i;
            if(temp<=citations[i])
            {
                return temp;
            }
        }
        return 0;
    }
}

